{
    "gitbook": ">=3.0.0",
    "title": "Practicalli Emacs Doom",
    "author": "Practicalli",
    "authorHomepage": "https://practical.li",
    "description": "Practical guide to REPL Driven Development with Emacs Doom",
    "structure": {
        "readme": "introduction.md"
    },
    "styles": {
        "website": "styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
    },
    "plugins": [
        "anchor-headings-lambda",
        "callouts",
        "codeblock-disable-glossary",
        "disqus",
        "folding-chapters",
        "ga",
        "-glossary",
        "-highlight",
        "klipse",
        "newtabs",
        "prism",
        "sectionx",
        "simpletabs",
        "-sharing",
        "splitter",
        "terminull-light",
        "toggle-chapters",
        "toolbar",
        "youtube",
        "wide-page"
    ],
    "pluginsConfig": {
        "callouts": {
            "showTypeInHeader": false,
            "star": {
                "alert": "warning",
                "picto": "fa-star"
            }
        },
        "prism": {
            "ignore": [
                "eval-clojure"
            ]
        },
        "disqus": {
            "shortName": "clojure-practicalli"
        },
        "ga": {
            "token": "UA-28030709-7"
        },
        "toolbar": {
            "buttons":
            [
                {
                   "label": "Clojurians Slack",
                   "icon": "fa fa-comments-o",
                   "url": "http://clojurians.slack.com/messages/practicalli"
                },
                {
                    "label": "GitHub",
                    "icon": "fa fa-github",
                    "url": "https://github.com/practicalli/doom-emacs/issues"
                }
            ]
        }
    },
    "links": {
        "sidebar": {
            "Practicalli": "https://practical.li/"
        }
    },
    "variables": {
        "website": "https://practical.li/doom-emacs",
        "P9IDeveloperGuides": "https://github.com/practicalli-john/developer-guides/",
        "P9IClojureDepsEdn": "https://github.com/practicalli/clojure-deps-edn",
        "P9IClojureDepsEdnInstall": "https://practical.li/clojure/clojure-tools/install/community-tools.md",
        "P9IFourClojure": "https://github.com/practicalli/four-clojure",
        "P9IClojureThroughCode": "https://github.com/practicalli/clojure-through-code",
        "P9IIssuesUrl": "https://github.com/practicalli/doom-emacs/issues",
        "P9IPullRequestsUrl": "https://github.com/practicalli/doom-emacs/pulls",
        "Leiningen": "https://leiningen.org/",
        "KeyCtrl": "<kbd>Ctrl</kbd>",
        "KeyEnter": "<kbd>Enter</kbd>",
        "KeyEsc": "<kbd>Esc</kbd>",
        "KeyUpArrow": "<kbd>↑</kbd>",
        "KeyDownArrow": "<kbd>↓</kbd>"
    }
}
